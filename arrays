https://leetcode.com/problems/maximum-subarray/
class Solution {
    public int maxSubArray(int[] nums) {

         int maxSum =0;
         int currentsum=0;
    for(int i=0; i<nums.length; i++){
        for(int j=i; j< nums.length;j++){
             currentsum+=nums[j];
            if(currentsum>maxSum){
                maxSum=currentsum;
            }
            //if(currentsum<0){currentsum=0;}
        }
        currentsum=0;
    }
    return maxSum;
    }

}
