class Solution {
    public int strStr(String haystack, String needle) {
        int ini=haystack.length();  
        int fin= needle.length(); 
//02 13 25 36
for(int i=0, j=fin ; i<= ini-fin; i++, j++){
    System.out.println(haystack.substring(i,j));
    System.out.println(needle);
    if((haystack.substring(i,j)).equals(needle)){
        return i;
    }
}
return -1;

    }
}
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
class Solution {
    public int firstUniqChar(String s) {
        char[] c = s.toCharArray();
        Map<Character, Integer> mychar = new HashMap<>();
        for(int i=0; i<c.length; i++){
            int count = 0;
                if(mychar.get(c[i])!=null){
                     count = mychar.get(c[i]);
                    mychar.put(c[i], count+1);
                    //return i;
                }else{
                    mychar.put(c[i], 1);
                     System.out.println("first "+c[i]);
                }
             
        }
        for(int i=0; i<c.length; i++ ){
            if( mychar.get(c[i]) ==1 ){
                return i;
            }
        }
        return -1;
    }
}
